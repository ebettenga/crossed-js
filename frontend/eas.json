{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
       "ios": {
        "simulator": true
      },
      "developmentClient": true,
      "distribution": "internal"
    },

    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
"submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./eas-build-key.json",
        "track": "internal"
      },
      "ios": {
        "ascAppId": "your-app-store-connect-app-id"
      }
    }
  }
}
