{
  "name": "crossed",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.ts",
  "exports": "./dist/index.mjs",
  "scripts": {
    "start": "node ./dist/index.mjs",
    "build": "pkgroll",
    "dev": "tsx --watch ./src/index.ts",
    "commands": "tsx ./src/commands/cli.ts",
    "prepare": "if [ \"$CI\" != \"true\" ]; then pre-commit install; fi",
    "ci:load-test-data": "yarn commands load-test-data",
    "test": "tsx --experimental-vm-modules node_modules/jest/bin/jest.js --verbose --coverage",
    "serve:coverage": "npx serve coverage/lcov-report",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate",
    "migration:run": "typeorm-ts-node-commonjs migration:run",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert"
  },
  "dependencies": {
    "@fastify/autoload": "^6.0.2",
    "@fastify/basic-auth": "^6.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/passport": "^3.0.1",
    "@fastify/secure-session": "^8.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "bcrypt": "^5.1.1",
    "fastify": "^5.0.0",
    "fastify-socket.io": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "passport-github": "^1.1.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "socket.io": "^4.0.0",
    "typeorm": "^0.3.20",
    "typeorm-fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^22.10.1",
    "@types/passport": "^1.0.17",
    "commander": "^12.1.0",
    "jest": "^29.0.0",
    "pino-pretty": "^13.0.0",
    "pkgroll": "^2.5.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
