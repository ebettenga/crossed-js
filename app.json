{
  "expo": {
    "name": "Crossed",
    "slug": "crossed",
    "android": {
      "package": "com.crossed.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "ios": {
      "bundleIdentifier": "com.crossed.app",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        },
        "expo-secure-store"
      ]
    ]
  }
}
